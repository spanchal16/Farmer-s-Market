<!-- Displaying error message -->

<% if(typeof err != "undefined"){ %>
<div class="mb-3" style="margin-top: 45vh; text-align: center;">
  <h3>Error</h3>
  <h5>
    <%= err.message %>
  </h5>
</div>
<%}else{%>
<div class="mb-3" style="margin-top: 50vh; text-align: center;">
  <h3>Page Not Found</h3>
</div>
<%}%>
