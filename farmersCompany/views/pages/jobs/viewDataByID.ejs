<style>
    table,
    th,
    td {
        padding: 10px;
        border: 1px solid black;
        border-collapse: collapse;
    }
</style>

<body>
    <h1> View Data By ID for Jobs Table </h1>
    <br>
    <table>
        <thread>
            <tr>
                <th> Job Name </th>
                <th> Part ID </th>
                <th> Quantity </th>
            </tr>
        </thread>
        <tbody>
            <tr>
                <% Object.keys(job).forEach(function(key) { %>
                <td> <%= job[key] %> </td>
                <% }) %>
            </tr>
        </tbody>
    </table>
</body>
<br>
<form action="/">
    <button> Home </button>
</form>
<br>
<form action="/jobs/viewData">
    <button> View Data </button>
</form>